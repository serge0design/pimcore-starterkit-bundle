{
  "name": "serge0design/pimcore-starterkit-bundle",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "keywords": [
    "pimcore",
    "starterkit",
    "boilerplate",
    "template"
  ],
  "description": "This Pimcore Bundle copies some files to setup a pimcore boilerplate template. This bundle requires a fresh pimcore skeleton base Installation to be installed.",
  "homepage": "https://github.com/serge0design/pimcore-starterkit.git",
  "authors": [
    {
      "name": "serge.design - Serge",
      "email": "github@serge-design.ch",
      "homepage": "https://serge-design.ch",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~8.1.0 || ~8.2.0",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "pimcore/pimcore": "^v11.1.0",
    "pimcore/admin-ui-classic-bundle": "^1.0",
    "pimcore/bundle-generator": "^2.0",
    "pimcore/ecommerce-framework-bundle": "^1.0",
    "pimcore/file-explorer-bundle": "^1.0",
    "pimcore/google-marketing-bundle": "^1.0",
    "pimcore/newsletter-bundle": "^1.0",
    "pimcore/personalization-bundle": "^1.0",
    "pimcore/system-info-bundle": "^1.0",
    "pimcore/web-to-print-bundle": "^1.0",
    "symfony/dotenv": "^6.2",
    "symfony/runtime": "^6.2",
    "symfony/ux-twig-component": "^v2.3.0",
    "symfony/webpack-encore-bundle": "^v1.17.2",
    "twig/extra-bundle": "^v3.7.1",
    "twig/string-extra": "^v3.7.1",
    "dachcom-digital/formbuilder": "^v5.0.0",
    "pringuin/dataprivacybundle": "^v2.1"
  },
  "require-dev": {
    "codeception/codeception": "^5.0",
    "codeception/module-symfony": "^3.1",
    "codeception/module-webdriver": "^4.0",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-symfony": "^1.0",
    "symplify/easy-coding-standard": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "SergeDesign\\StarterkitBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SergeDesign\\StarterkitBundle\\": "src/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "SergeDesign\\StarterkitBundle\\PimcoreStarterkitBundle"
      ]
    }
  }
}

